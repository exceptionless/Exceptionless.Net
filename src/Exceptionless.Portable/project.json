                "System.Xml.ReaderWriter": "4.0.11-rc2-24027",
                "System.Xml.XDocument": "4.0.11-rc2-24027",
                "System.Xml.XmlSerializer":  "4.0.11-rc2-24027" 
            }
        },
        ".NETPortable,Version=v4.6,Profile=Profile151": {
            "buildOptions": {
                "define": [ "PORTABLE", "NETSTANDARD1_2" ]
